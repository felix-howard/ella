{
  "name": "@ella/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "generate": "dotenv -e ../../.env -- prisma generate",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy",
    "push": "dotenv -e ../../.env -- prisma db push",
    "studio": "dotenv -e ../../.env -- prisma studio",
    "seed": "dotenv -e ../../.env -- tsx prisma/seed.ts",
    "seed:test": "dotenv -e ../../.env -- tsx prisma/seed-test-data.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
