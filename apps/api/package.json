{
  "name": "@ella/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/dotenv-cli/cli.js -e .env -- tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --noExternal @ella/db --noExternal @ella/shared",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@clerk/backend": "^2.29.2",
    "@ella/db": "workspace:*",
    "@ella/shared": "workspace:*",
    "@google/generative-ai": "^0.21.0",
    "@hono/clerk-auth": "^3.0.3",
    "@hono/node-server": "^1.13.8",
    "@hono/zod-openapi": "=0.18.3",
    "@hono/zod-validator": "^0.7.6",
    "hono": "^4.6.15",
    "inngest": "^3.49.1",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "pdf-poppler": "^0.2.3",
    "sharp": "^0.34.5",
    "twilio": "^5.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "dotenv-cli": "^8.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.32.0",
    "@vitest/coverage-v8": "^4.0.17",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  }
}
